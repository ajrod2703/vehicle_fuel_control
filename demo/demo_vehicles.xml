<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Vehículos de demostración -->
        <record id="vehicle_demo_1" model="vehicle.fuel.vehicle">
            <field name="name">Toyota Hilux</field>
            <field name="license_plate">ABC-123</field>
            <field name="model">Hilux 4x4</field>
            <field name="driver_id" ref="base.partner_demo_portal"/> <!-- Usuario demo existente, ajusta si no existe -->
            <field name="active">True</field>
        </record>

        <record id="vehicle_demo_2" model="vehicle.fuel.vehicle">
            <field name="name">Ford Ranger</field>
            <field name="license_plate">XYZ-789</field>
            <field name="model">Ranger XLT</field>
            <field name="driver_id" ref="base.partner_demo"/> <!-- Otro usuario demo -->
            <field name="active">True</field>
        </record>

        <record id="vehicle_demo_3" model="vehicle.fuel.vehicle">
            <field name="name">Nissan Frontier</field>
            <field name="license_plate">LMN-456</field>
            <field name="model">Frontier SE</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>